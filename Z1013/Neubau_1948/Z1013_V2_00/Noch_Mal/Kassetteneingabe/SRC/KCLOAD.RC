	PUSHBUTTON "&Speichern auf Kassette...", 22, 2, 28, 92, 14
	RTEXT "&Lautstärke:", -1, 112, 30, 38, 8
	CONTROL "", 13, "EDIT", ES_LEFT | ES_AUTOHSCROLL | WS_CHILD | WS_VISIBLE | WS_BORDER | WS_VSCROLL | WS_TABSTOP, 152, 28, 26, 12
	LTEXT "&Kodierung:", -1, 4, 46, 38, 8
