	CONTROL "", 14, "COMBOBOX", CBS_DROPDOWNLIST | WS_CHILD | WS_VISIBLE | WS_GROUP | WS_TABSTOP, 18, 68, 120, 46
	LTEXT "Out", -1, 4, 82, 12, 8
	CONTROL "", 15, "COMBOBOX", CBS_DROPDOWNLIST | WS_CHILD | WS_VISIBLE | WS_GROUP | WS_TABSTOP, 18, 80, 120, 46
	CONTROL "Abbru&ch", 2, "BUTTON", BS_PUSHBUTTON | WS_CHILD | NOT WS_VISIBLE | WS_GROUP | WS_TABSTOP, 140, 44, 42, 14
	CONTROL "&Ende", 3, "BUTTON", BS_PUSHBUTTON | WS_CHILD | WS_VISIBLE | WS_GROUP | WS_TABSTOP, 140, 44, 42, 14
	PUSHBUTTON "&Hilfe", 9, 140, 60, 42, 14
	PUSHBUTTON "&Installieren", 23, 140, 76, 42, 14
	CONTROL "", 10, "STATIC", SS_LEFTNOWORDWRAP | SS_NOPREFIX | WS_CHILD | WS_VISIBLE | WS_BORDER | WS_GROUP, 0, 94, 184, 12
END


